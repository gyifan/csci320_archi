    %set/v v0x2992f20_0, 0, 8;
    %set/v v0x2992fe0_0, 0, 1;
    %end;
    .scope S_0x295f610;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_0x295f610;
T_1 ;
    %fork t_3, S_0x295f7f0;
    %jmp t_2;
    .scope S_0x295f7f0;
t_3 ;
    %set/v v0x295f8e0_0, 0, 32;
T_1.0 ;
    %load/v 8, v0x295f8e0_0, 32;
   %cmpi/s 8, 8, 32;
    %jmp/0xz T_1.1, 5;
    %movi 11, 7, 33;
    %load/v 44, v0x295f8e0_0, 32;
    %mov 76, 75, 1;
    %sub 11, 44, 33;
    %ix/get/s 1, 11, 33;
    %jmp/1 T_1.2, 4;
    %load/x1p 11, v0x2992f20_0, 1;
    %jmp T_1.3;
T_1.2 ;
    %mov 11, 2, 1;
T_1.3 ;
    %mov 8, 11, 1; Move signal select into place
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.4, 4;
    %load/v 8, v0x2992fe0_0, 1;
    %inv 8, 1;
    %set/v v0x2992fe0_0, 8, 1;
T_1.4 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x295f8e0_0, 32;
    %set/v v0x295f8e0_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %load/v 8, v0x2992fe0_0, 1;
    %jmp/0xz  T_1.6, 8;
    %vpi_call 2 24 "$display", "%4d, %b has ODD  parity (%b)", $time, v0x2992f20_0, v0x2992fe0_0;
    %jmp T_1.7;
T_1.6 ;
    %vpi_call 2 28 "$display", "%4d, %b has EVEN parity (%b)", $time, v0x2992f20_0, v0x2992fe0_0;
T_1.7 ;
    %delay 1, 0;
    %end;
    .scope S_0x295f610;
t_2 %join;
    %jmp T_1;
    .thread T_1;
    .scope S_0x295f610;
T_2 ;
    %fork t_5, S_0x295f700;
    %jmp t_4;
    .scope S_0x295f700;
t_5 ;
    %movi 8, 113, 8;
    %set/v v0x2992f20_0, 8, 8;
    %set/v v0x2992fe0_0, 0, 1;
    %delay 1, 0;
    %movi 8, 81, 8;
    %set/v v0x2992f20_0, 8, 8;
    %set/v v0x2992fe0_0, 0, 1;
    %delay 1, 0;
    %set/v v0x2992f20_0, 1, 8;
    %set/v v0x2992fe0_0, 0, 1;
    %delay 1, 0;
    %movi 8, 127, 8;
    %set/v v0x2992f20_0, 8, 8;
    %set/v v0x2992fe0_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 43 "$finish";
    %end;
