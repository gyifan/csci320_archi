#! /usr/bin/vvp
:ivl_version "0.9.6 " "(v0_9_6)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x200a810 .scope module, "simple" "simple" 2 4;
 .timescale 0 0;
v0x200abd0_0 .var "A", 7 0;
v0x203cb30_0 .var "B", 7 0;
v0x203cbd0_0 .var "C", 0 0;
S_0x200aae0 .scope begin, "stop_at" "stop_at" 2 15, 2 15, S_0x200a810;
 .timescale 0 0;
S_0x200a9f0 .scope begin, "Init" "Init" 2 21, 2 21, S_0x200a810;
 .timescale 0 0;
S_0x200a900 .scope begin, "main_process" "main_process" 2 35, 2 35, S_0x200a810;
 .timescale 0 0;
    .scope S_0x200a810;
T_0 ;
    %fork t_1, S_0x200aae0;
    %jmp t_0;
    .scope S_0x200aae0;
t_1 ;
    %delay 20, 0;
    %vpi_call 2 17 "$finish";
    %end;
    .scope S_0x200a810;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_0x200a810;
T_1 ;
    %fork t_3, S_0x200a9f0;
    %jmp t_2;
    .scope S_0x200a9f0;
t_3 ;
    %set/v v0x200abd0_0, 0, 8;
    %vpi_call 2 24 "$dumpfile", "simple.vcd";
    %vpi_call 2 25 "$dumpvars", 1'sb0, S_0x200a810;
    %vpi_call 2 28 "$display", "Time   A         B    C";
    %vpi_call 2 31 "$monitor", "  %0d %b %b %b", $time, v0x200abd0_0, v0x203cb30_0, v0x203cbd0_0;
    %end;
    .scope S_0x200a810;
t_2 %join;
    %end;
    .thread T_1;
    .scope S_0x200a810;
T_2 ;
    %fork t_5, S_0x200a900;
    %jmp t_4;
    .scope S_0x200a900;
t_5 ;
    %delay 1, 0;
    %load/v 8, v0x200abd0_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %set/v v0x200abd0_0, 8, 8;
    %delay 1, 0;
    %load/v 8, v0x200abd0_0, 4; Only need 4 of 8 bits
; Save base=8 wid=4 in lookaside.
    %inv 8, 4;
    %ix/load 0, 4, 0;
    %set/x0 v0x203cb30_0, 8, 4;
    %delay 1, 0;
    %load/v 8, v0x200abd0_0, 2; Only need 2 of 8 bits
; Save base=8 wid=2 in lookaside.
    %and/r 8, 8, 2;
    %set/v v0x203cbd0_0, 8, 1;
    %end;
    .scope S_0x200a810;
t_4 %join;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "simple.v";
