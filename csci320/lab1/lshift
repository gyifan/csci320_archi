#! /usr/bin/vvp
:ivl_version "0.9.6 " "(v0_9_6)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x17645b0 .scope module, "lshift" "lshift" 2 1;
 .timescale 0 0;
v0x1796a00_0 .var "A", 7 0;
S_0x1796910 .scope begin, "stop_at" "stop_at" 2 5, 2 5, S_0x17645b0;
 .timescale 0 0;
S_0x1796820 .scope begin, "Init" "Init" 2 11, 2 11, S_0x17645b0;
 .timescale 0 0;
S_0x17646a0 .scope begin, "main_process" "main_process" 2 25, 2 25, S_0x17645b0;
 .timescale 0 0;
v0x1764790_0 .var/i "i", 31 0;
    .scope S_0x17645b0;
T_0 ;
    %fork t_1, S_0x1796910;
    %jmp t_0;
    .scope S_0x1796910;
t_1 ;
    %delay 25, 0;
    %vpi_call 2 7 "$finish";
    %end;
    .scope S_0x17645b0;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_0x17645b0;
T_1 ;
    %fork t_3, S_0x1796820;
    %jmp t_2;
    .scope S_0x1796820;
t_3 ;
    %movi 8, 1, 8;
    %set/v v0x1796a00_0, 8, 8;
    %vpi_call 2 14 "$dumpfile", "lshift.vcd";
    %vpi_call 2 15 "$dumpvars", 1'sb0, S_0x17645b0;
    %vpi_call 2 18 "$display", "Time   A";
    %vpi_call 2 21 "$monitor", "  %0d %b", $time, v0x1796a00_0;
    %end;
    .scope S_0x17645b0;
t_2 %join;
    %end;
    .thread T_1;
    .scope S_0x17645b0;
T_2 ;
    %fork t_5, S_0x17646a0;
    %jmp t_4;
    .scope S_0x17646a0;
t_5 ;
T_2.0 ;
    %movi 8, 1, 2;
    %or/r 8, 8, 2;
    %jmp/0xz T_2.1, 8;
    %movi 8, 1, 8;
    %set/v v0x1796a00_0, 8, 8;
    %set/v v0x1764790_0, 0, 32;
T_2.2 ;
    %load/v 8, v0x1764790_0, 32;
   %cmpi/s 8, 8, 32;
    %jmp/0xz T_2.3, 5;
    %delay 1, 0;
    %load/v 8, v0x1796a00_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 8;
    %set/v v0x1796a00_0, 8, 8;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x1764790_0, 32;
    %set/v v0x1764790_0, 8, 32;
    %jmp T_2.2;
T_2.3 ;
    %jmp T_2.0;
T_2.1 ;
    %end;
    .scope S_0x17645b0;
t_4 %join;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
