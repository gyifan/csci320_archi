    %load/v 8, v0x24748c0_0, 8;
    %set/v v0x244e6f0_0, 8, 8;
    %delay 1, 0;
    %jmp T_2.8;
T_2.5 ;
    %load/v 8, v0x244e6f0_0, 8;
    %set/v v0x24748c0_0, 8, 8;
    %load/v 8, v0x24748c0_0, 8;
    %ix/getv 3, v0x2474810_0;
   %jmp/1 t_6, 4;
   %ix/load 1, 0, 0;
   %set/av v0x2474960, 8, 8;
t_6 ;
    %delay 1, 0;
    %jmp T_2.8;
T_2.6 ;
    %ix/getv 3, v0x2474810_0;
    %load/av 8, v0x2474960, 8;
    %set/v v0x24748c0_0, 8, 8;
    %delay 1, 0;
