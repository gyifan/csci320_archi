    %load/v 16, v0x24748c0_0, 8;
    %add 8, 16, 8;
    %set/v v0x24746d0_0, 8, 8;
    %delay 1, 0;
    %load/v 8, v0x24746d0_0, 8;
    %set/v v0x244e6f0_0, 8, 8;
