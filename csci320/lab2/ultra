:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x244e330 .scope module, "ultra" "ultra" 2 8;
 .timescale 0 0;
v0x244e6f0_0 .var "AC", 7 0;
v0x24746d0_0 .var "ALU", 7 0;
v0x2474770_0 .var "IR", 7 0;
v0x2474810_0 .var "MA", 7 0;
v0x24748c0_0 .var "MD", 7 0;
v0x2474960 .array "MEM", 63 0, 7 0;
v0x2474a20_0 .var "PC", 7 0;
v0x2474ac0_0 .var "opcode", 1 0;
S_0x244e600 .scope begin, "stop_at" "stop_at" 2 15, 2 15, S_0x244e330;
 .timescale 0 0;
S_0x244e510 .scope begin, "Init" "Init" 2 21, 2 21, S_0x244e330;
 .timescale 0 0;
S_0x244e420 .scope begin, "main_process" "main_process" 2 58, 2 58, S_0x244e330;
 .timescale 0 0;
    .scope S_0x244e330;
T_0 ;
    %fork t_1, S_0x244e600;
    %jmp t_0;
    .scope S_0x244e600;
t_1 ;
    %delay 100, 0;
    %vpi_call 2 17 "$finish";
    %end;
    .scope S_0x244e330;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_0x244e330;
T_1 ;
    %fork t_3, S_0x244e510;
    %jmp t_2;
    .scope S_0x244e510;
t_3 ;
    %movi 8, 16, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v0x2474960, 8, 8;
    %movi 8, 82, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 1, 0;
   %set/av v0x2474960, 8, 8;
    %movi 8, 145, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 2, 0;
   %set/av v0x2474960, 8, 8;
    %movi 8, 193, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 3, 0;
   %set/av v0x2474960, 8, 8;
    %movi 8, 5, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 16, 0;
   %set/av v0x2474960, 8, 8;
    %movi 8, 1, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 17, 0;
   %set/av v0x2474960, 8, 8;
   %ix/load 1, 0, 0;
   %ix/load 3, 18, 0;
   %set/av v0x2474960, 0, 8;
    %set/v v0x244e6f0_0, 0, 8;
    %set/v v0x2474770_0, 0, 8;
    %set/v v0x24748c0_0, 0, 8;
    %set/v v0x2474a20_0, 0, 8;
    %set/v v0x2474810_0, 0, 8;
    %set/v v0x24746d0_0, 0, 8;
    %set/v v0x2474ac0_0, 0, 2;
    %vpi_call 2 45 "$display", "Time\011PC\011opcode\011IR\011AC\011MD\011MA\011ALU\011OUTPUT";
    %vpi_call 2 48 "$monitor", "\011%0d\011%b\011%b\011%b\011%b\011%b\011%b\011%b\011%0d", $time, v0x2474a20_0, v0x2474ac0_0, v0x2474770_0, v0x244e6f0_0, v0x24748c0_0, v0x2474810_0, v0x24746d0_0, &A<v0x2474960, 18>;
    %vpi_call 2 51 "$dumpfile", "ultra.vcd";
    %vpi_call 2 52 "$dumpvars", 1'sb0, S_0x244e330;
    %end;
    .scope S_0x244e330;
t_2 %join;
    %end;
    .thread T_1;
    .scope S_0x244e330;
T_2 ;
    %fork t_5, S_0x244e420;
    %jmp t_4;
    .scope S_0x244e420;
t_5 ;
T_2.0 ;
    %movi 8, 1, 2;
    %or/r 8, 8, 2;
    %jmp/0xz T_2.1, 8;
    %ix/getv 3, v0x2474a20_0;
    %load/av 8, v0x2474960, 8;
    %set/v v0x2474770_0, 8, 8;
    %load/v 8, v0x2474a20_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %set/v v0x2474a20_0, 8, 8;
    %delay 1, 0;
    %ix/load 1, 6, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.2, 4;
    %load/x1p 8, v0x2474770_0, 2;
    %jmp T_2.3;
T_2.2 ;
    %mov 8, 2, 2;
T_2.3 ;
; Save base=8 wid=2 in lookaside.
    %set/v v0x2474ac0_0, 8, 2;
    %load/v 8, v0x2474770_0, 6; Select 6 out of 8 bits
    %mov 14, 0, 2;
    %set/v v0x2474810_0, 8, 8;
    %delay 1, 0;
    %load/v 8, v0x2474ac0_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_2.4, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_2.5, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_2.6, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_2.7, 6;
    %jmp T_2.8;
T_2.4 ;
    %ix/getv 3, v0x2474810_0;
    %load/av 8, v0x2474960, 8;
    %set/v v0x24748c0_0, 8, 8;
